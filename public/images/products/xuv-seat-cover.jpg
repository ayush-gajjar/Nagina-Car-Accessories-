<svg width="800" height="800" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="seatGrad" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#95a5a6;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#7f8c8d;stop-opacity:1" />
    </linearGradient>
    <filter id="shadow"><feDropShadow dx="0" dy="6" stdDeviation="10" flood-opacity="0.3"/></filter>
  </defs>
  
  <rect width="800" height="800" fill="#ffffff"/>
  
  <!-- Seat backrest -->
  <rect x="200" y="150" width="400" height="450" rx="30" fill="url(#seatGrad)" filter="url(#shadow)"/>
  <rect x="220" y="170" width="360" height="410" rx="20" fill="#bdc3c7"/>
  
  <!-- Seat cushion -->
  <ellipse cx="400" cy="650" rx="180" ry="80" fill="url(#seatGrad)" filter="url(#shadow)"/>
  <ellipse cx="400" cy="645" rx="160" ry="70" fill="#bdc3c7"/>
  
  <!-- Stitching pattern -->
  <line x1="300" y1="200" x2="300" y2="550" stroke="#7f8c8d" stroke-width="3" stroke-dasharray="20,10"/>
  <line x1="400" y1="200" x2="400" y2="550" stroke="#7f8c8d" stroke-width="3" stroke-dasharray="20,10"/>
  <line x1="500" y1="200" x2="500" y2="550" stroke="#7f8c8d" stroke-width="3" stroke-dasharray="20,10"/>
  
  <!-- Logo area -->
  <rect x="330" y="320" width="140" height="60" rx="10" fill="#2c3e50"/>
  <text x="400" y="360" font-family="Arial" font-size="32" font-weight="bold" fill="white" text-anchor="middle">XUV 700</text>
  
  <!-- Texture dots -->
  <circle cx="250" cy="250" r="4" fill="#95a5a6"/>
  <circle cx="280" cy="250" r="4" fill="#95a5a6"/>
  <circle cx="250" cy="290" r="4" fill="#95a5a6"/>
  <circle cx="280" cy="290" r="4" fill="#95a5a6"/>
  <circle cx="520" cy="250" r="4" fill="#95a5a6"/>
  <circle cx="550" cy="250" r="4" fill="#95a5a6"/>
  <circle cx="520" cy="290" r="4" fill="#95a5a6"/>
  <circle cx="550" cy="290" r="4" fill="#95a5a6"/>
</svg>
